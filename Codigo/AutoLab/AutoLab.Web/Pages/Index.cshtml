@page
@model IndexModel
@{
    ViewData["Title"] = "Brand";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Brand</th>
            <th scope="col">Updated</th>
            <th scope="col">View</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Models)
        {
            <tr>
                <td>@item.Brand</td>
                <td>@item.Updated</td>
                <td>
                    <a href="~/Details?brand=@item.Key">
                        <i class="fa-solid fa-up-right-from-square"></i>
                    </a>
                </td>
            </tr>

        }

    </tbody>
</table>
